    "class InputData:\n",
    "    \n",
    "    def getDataFromCsv(self, csv_file, train = 1, icol = None):\n",
    "        #just a comment
    "        self.df = pd.DataFrame(pd.read_csv(csv_file, index_col = icol));\n",
    "        self.df_coded = self.df;\n",
    "        dim = np.shape(self.df);\n",
    "        self.nb_instances = dim[0];\n",
    "        self.train = train;\n",
    "        self.train_index = np.random.choice(self.nb_instances,int(self.train*self.nb_instances),replace = False);\n",
    "        aux_test_index = np.arange(self.nb_instances);\n",
    "        self.test_index = np.array([t for t in aux_test_index if t not in self.train_index]);\n",
    "    \n",
    "    def printData(self):\n",
    "        print(self.df);\n",
    "        \n",
    "    def describeData(self):\n",
    "        print(self.df_coded.describe());\n",
    "        \n",
    "    def scatterData(self):\n",
    "        pd.plotting.scatter_matrix(self.df,figsize=(10,10));\n",
    "        \n",
    "    def dataColumns(self):\n",
    "        return self.df.columns;\n",
    "        \n",
    "    def dataColumnsTypes(self):\n",
    "        print(self.df.dtypes);\n",
    "        \n",
    "    def dataCoded(self):\n",
    "        self.df = self.df.astype('category');\n",
    "        cat_columns = self.df.select_dtypes(['category']).columns;\n",
    "        self.df_coded = self.df[cat_columns].apply(lambda x: x.cat.codes);\n",
    "        \n",
    "    def dataDecode(self):\n",
    "        self.df = self.df.astype('category');\n",
    "        cat_columns = self.df.select_dtypes(['category']).columns;\n",
    "        d = dict(enumerate(self.df[t].cat.categories for t in cat_columns));\n",
    "        return d;\n",
    "        \n",
    "    def chooseTrainResp(self, col = 1):\n",
    "        return self.df_coded.iloc[self.train_index,col-1].values;\n",
    "    \n",
    "    def chooseTrainPred(self, col = []):\n",
    "        col = np.array([col]).flatten()\n",
    "        return self.df_coded.iloc[self.train_index,[t - 1 for t in col]].values;\n",
    "        \n",
    "    def chooseTestResp(self, col = 1):\n",
    "        return self.df_coded.iloc[self.test_index,col-1].values;\n",
    "    \n",
    "    def chooseTestPred(self, col = []):\n",
    "        col = np.array([col]).flatten()\n",
    "        return self.df_coded.iloc[self.test_index,[t - 1 for t in col]].values;\n",
    "    \n",
    "    def sortData(self, name_col):\n",
    "        print(self.df.sort_values(by=name_col,ascending=1));\n",
    "            \n",
