    "class Informations:\n",
    "    def __init__(self, X, y):\n",
    "        self.X = X;\n",
    "        self.y = y;\n",
    "        \n",
    "        dim = np.shape(self.X);\n",
    "        self.n = dim[1];\n",
    "        self.m = len(self.y);\n",
    "        \n",
    "        self.root = 0;\n",
    "    \n",
    "    def entropy(self,target):\n",
    "        entropy = 0;\n",
    "        unique_values = np.unique(target);\n",
    "        count_target = len(target);\n",
    "        \n",
    "        for t in unique_values:\n",
    "            target_t = target[target == t];\n",
    "            count_specific_target = len(target_t);\n",
    "            \n",
    "            if(count_specific_target == 0):\n",
    "                entropy = 0;\n",
    "                break;\n",
    "            else:\n",
    "                entropy += (count_specific_target/count_target) * math.log(count_target/count_specific_target,2);\n",
    "        \n",
    "        return entropy;\n",
    "    \n",
    "    def information_gain(self,target,entropy_branch,indexes_parent):\n",
    "        ig = entropy_branch;\n",
    "        unique_branches = np.unique(target);\n",
    "        count_target = len(target);\n",
    "        \n",
    "        for branch in unique_branches:\n",
    "            indexes_branch = np.array(np.where(target==branch)).flatten(); #pozitiile unde apar elemente din ramura respectiva\n",
    "            indexes = [value for value in indexes_branch if value in indexes_parent];\n",
    "            \n",
    "            y_branch = np.array([self.y[t] for t in indexes]).flatten(); #valorile returnate pentru ramura respectiva\n",
    "            count_y_branch = len(y_branch);\n",
    "            \n",
    "            entropy_branch = self.entropy(y_branch);\n",
    "            ig = ig - (count_y_branch/count_target)*entropy_branch;\n",
    "            \n",
    "        return ig;\n",
    "    \n",
    "    def showIG(self,IG):\n",
    "        i = 1;\n",
    "        for x in IG:\n",
    "            print(str('%.4f'%x),end=\" \");\n",
    "            if(i%3 == 0):\n",
    "                print(\"\");\n",
    "            i = i+1;\n",
    "        \n",
    "    def getRoot(self):\n",
    "                \n",
    "        IG = np.zeros(self.n);\n",
    "        entropy_y = self.entropy(self.y);\n",
    "        \n",
    "        for i in np.array(range(self.n)):\n",
    "            x = self.X[:,i];\n",
    "            IG[i] = self.information_gain(x,entropy_y,range(self.m));\n",
    "            \n",
    "        #self.showIG(IG);\n",
    "        \n",
    "        return (np.argmax(IG),np.max(IG));\n",
    "\n",
    "        \n",
    "    def constructTreeArray(self):\n",
    "        root = self.getRoot();\n",
    "        self.num_nodes = 1;\n",
    "        search_domain = np.array(range(self.n)); #domeniul de cautare pentru nodurile descendente\n",
    "        search_domain[root[0]] = -1; #nu mai caut si nodul radacina\n",
    "        \n",
    "        root_node = TreeNode([root[0],self.y,range(self.m),0,search_domain,root[1]]);\n",
    "        stack = [root_node];\n",
    "        \n",
    "        #cat timp stiva e nevida\n",
    "        while(len(stack) != 0):\n",
    "            \n",
    "            #luam parintele actual din arbore\n",
    "            actual_parent = stack.pop();\n",
    "            \n",
    "            index_actual_parent = actual_parent.getColumnIndex();\n",
    "            labels_actual_parent = actual_parent.getLabels();\n",
    "            indexes_labels_values_parent = actual_parent.getIndexesLabels();\n",
    "            search_domain = actual_parent.getSearchDomain();\n",
    "            \n",
    "            #selectam datele vectorului parintelui actual\n",
    "            data_actual_parent = self.X[:,index_actual_parent];\n",
    "            \n",
    "            #alegem doar valorile unice din date(ramurile pe care poate merge)\n",
    "            unique_data_actual_parent = np.array(np.unique(data_actual_parent)).flatten();\n",
    "            \n",
    "            #pentru fiecare ramura a nodului actual\n",
    "            for xk in unique_data_actual_parent:\n",
    "                new_search_domain = np.copy(search_domain); #facem o copie a domeniului de cautare\n",
    "                \n",
    "                #alegem acele elemente din y cu care sunt corespunzatoare ramurii X = Xk \n",
    "                indexes_xk = np.array(np.where(data_actual_parent == xk)).flatten();\n",
    "                y_indexes_xk = np.intersect1d(indexes_xk,indexes_labels_values_parent);\n",
    "                \n",
    "                y_values_xk = np.array([self.y[t] for t in y_indexes_xk]).flatten();\n",
    "                if(len(y_values_xk) == 0):\n",
    "                    continue;\n",
    "                \n",
    "                #calculam entropia pe aceasta ramura\n",
    "                entropy_y_xk = self.entropy(y_values_xk);\n",
    "                \n",
    "                \n",
    "                #daca entropia pe ramura e nula, tragem concluzia ca pe ramura respectiva exista o singura valoare\n",
    "                if(entropy_y_xk == 0):\n",
    "                    nou = TreeNode([-y_values_xk[0]-1,y_values_xk,indexes_xk,xk,0,0]); #codam si punem in nodul ce succede ramura\n",
    "                                                                        #valoarea din y corespunzatoare\n",
    "                else:\n",
    "                    \n",
    "                    IG = np.zeros(self.n);\n",
    "                    #daca entropia nu este nula pe ramura, atunci\n",
    "                    #parcurgem domeniul de cautare a ramurii respective(ce atribute sunt disponibile pentru a putea\n",
    "                    #fi folosite ca nod curent)\n",
    "                    for i in search_domain[search_domain > -1]:\n",
    "                        \n",
    "                        #luam valorile corespunzatoare atributului i\n",
    "                        actual_node = self.X[:,i];\n",
    "                        \n",
    "                        IG[i] = self.information_gain(actual_node, entropy_y_xk, y_indexes_xk);\n",
    "                        \n",
    "                    if(np.count_nonzero(IG) > 0):\n",
    "                        index_of_max_IG = np.argmax(IG);\n",
    "                        value_of_max_IG = np.max(IG);\n",
    "                        \n",
    "                        new_search_domain[index_of_max_IG] = -1;\n",
    "                        \n",
    "                        nou = TreeNode([index_of_max_IG,y_values_xk,y_indexes_xk,xk,new_search_domain,value_of_max_IG]);\n",
    "                        stack.append(nou);\n",
    "                    else:\n",
    "                        aux = np.zeros(max(y_values_xk)+1);\n",
    "                        \n",
    "                        for yt in range(max(y_values_xk)+1):\n",
    "                            aux[yt] = len(y_values_xk[y_values_xk==yt]);\n",
    "                            \n",
    "                        p = np.argmax(aux);\n",
    "                        nou = TreeNode([-p-1,y_values_xk,y_indexes_xk,xk,0,0]);\n",
    "                    \n",
    "                self.num_nodes += 1;    \n",
    "                actual_parent.add_child(nou);\n",
    "                \n",
    "        self.root = root_node;\n",
    "        return root_node;\n",
    "\n",
    "    def chipruning(self,p):\n",
    "        \n",
    "        chi = 0;\n",
    "        pvalue = 0;\n",
    "        N = 0;\n",
    "        \n",
    "        k = 0;\n",
    "        Q = queue.Queue(maxsize=200);\n",
    "        Q.put(self.root);\n",
    "        \n",
    "        while(Q.qsize() != 0):\n",
    "            actual = Q.get();\n",
    "            \n",
    "            if(actual.getColumnIndex() >= 0):\n",
    "                index_actual = actual.getColumnIndex();\n",
    "                indexes_labels_values = actual.getIndexesLabels();\n",
    "                labels_values = actual.getLabels();\n",
    "                data_actual = self.X[:,index_actual];\n",
    "                unique_data_actual = np.array(np.unique(data_actual)).flatten();\n",
    "                unique_label_values = np.array(np.unique(self.y)).flatten();\n",
    "                \n",
    "                N = len(indexes_labels_values);\n",
    "                \n",
    "                o = np.zeros((len(unique_data_actual),len(unique_label_values)));                \n",
    "                e = np.zeros((len(unique_data_actual),len(unique_label_values)));\n",
    "                \n",
    "                for i in unique_data_actual:\n",
    "                    indexes_i = np.array(np.where(data_actual == i)).flatten();\n",
    "                    indexes = np.intersect1d(indexes_i,indexes_labels_values);\n",
    "\n",
    "                    for j in unique_label_values:\n",
    "                        indexes_j = np.array(np.where(self.y == j)).flatten();\n",
    "                        indexes = np.intersect1d(indexes_i,indexes_j);\n",
    "                        indexes = np.intersect1d(indexes,indexes_labels_values);\n",
    "\n",
    "                        label_values = np.array([self.y[t] for t in indexes]).flatten();\n",
    "\n",
    "                        o[i][j] = len(label_values);\n",
    "                \n",
    "                colsumo = o.sum(axis = 0);\n",
    "                linsumo = o.sum(axis = 1);\n",
    "                \n",
    "                for i in unique_data_actual:\n",
    "                    for j in unique_label_values:\n",
    "                        e[i][j] = linsumo[i]*colsumo[j]/N;\n",
    "                        \n",
    "                chi_aux = pow((o-e),2)/e;\n",
    "                chi = chi_aux.sum(axis = 0).sum();\n",
    "                df = (len(unique_data_actual)-1)*(len(unique_label_values)-1);\n",
    "                \n",
    "                pvalue = 1 - stats.chi2.cdf(chi , df);\n",
    "                \n",
    "                print('O: \\n',o);\n",
    "                print('E: \\n',e);\n",
    "                print('chi: ',chi);\n",
    "                print('p-value: ',pvalue);\n",
    "                print(\"\")\n",
    "                \n",
    "                if(pvalue > p):\n",
    "                    aux = np.zeros(max(labels_values)+1);\n",
    "                        \n",
    "                    for yt in range(max(labels_values)+1):\n",
    "                        aux[yt] = len(labels_values[labels_values==yt]);\n",
    "\n",
    "                    p = np.argmax(aux);\n",
    "                    actual.setColumnIndex(-p-1);\n",
    "                    actual.setIG(0);\n",
    "                    actual.children = [];\n",
    "                    \n",
    "                for t in actual.children:\n",
    "                    Q.put(t);\n",
    "                k += 1;\n",
    "        \n",
    "      \n",
    "    def predict(self,x):\n",
    "        pas = self.root;\n",
    "        while(len(pas.children) != 0):\n",
    "            try:\n",
    "                #print(dc[pas.getColumnIndex()],end = \" --\");\n",
    "                pas = pas.children[x[pas.getColumnIndex()]];\n",
    "                #print(str(d[1][pas.getBranch()]),end = \"--> \");\n",
    "            except:\n",
    "                print(\"Something went wrong\");\n",
    "                return;\n",
    "                    \n",
    "        return -pas.getColumnIndex()-1;\n",
    "    \n",
    "class InformationsTest:\n",
    "    def __init__(self):\n",
    "        data = InputData();\n",
    "        data.getDataFromCsv('DateDT/kpkr.csv',train = .75, icol = 0);\n",
    "        data.dataCoded();\n",
    "        y_train = data.chooseTrainResp(37);\n",
    "        x_train = data.chooseTrainPred(np.array(range(1,37)));\n",
    "        \n",
    "        y_test = data.chooseTestResp(37);\n",
    "        x_test = data.chooseTestPred(np.array(range(1,37)));\n",
    "        \n",
    "        decode = data.dataDecode();\n",
    "        \n",
    "        info = Informations(x_train,y_train);\n",
    "        \n",
    "        tree = info.constructTreeArray();\n",
    "        #info.chipruning(0.05);\n",
    "        tree.showRender(data.dataColumns(),data.dataDecode(),2,\"tree-75.png\");\n",
    "        \n",
    "        p = 0;\n",
    "        for (xt,yt) in zip(x_test,y_test):\n",
    "            yp = info.predict(xt);\n",
    "            if yp == yt:\n",
    "                p = p + 1;\n",
    "        \n",
    "        print(p/len(x_test));\n",
    "\n",
    "InformationsTest();"
