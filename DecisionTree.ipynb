    "def nodenamefunc(node):\n",
    "    return'%s' % (node.name);\n",
    "\n",
    "def edgeattrfunc(node, child):\n",
    "    return'label=\"%s\"' % (child.edge);   \n",
    "                \n",
    "     \n",
    "class TreeNode:\n",
    "    def __init__(self,data):\n",
    "        self.data = data;\n",
    "        self.children = [];\n",
    "        \n",
    "    def add_child(self, obj):\n",
    "        self.children.append(obj);\n",
    "        \n",
    "    def getColumnIndex(self):\n",
    "        return self.data[0];\n",
    "    \n",
    "    def setColumnIndex(self, index):\n",
    "        self.data[0] = index;\n",
    "    \n",
    "    def getLabels(self):\n",
    "        return self.data[1];\n",
    "    \n",
    "    def getIndexesLabels(self):\n",
    "        return self.data[2];\n",
    "    \n",
    "    def getBranch(self):\n",
    "        return self.data[3];\n",
    "    \n",
    "    def getSearchDomain(self):\n",
    "        return self.data[4];\n",
    "    \n",
    "    def getIG(self):\n",
    "        return self.data[5];\n",
    "    \n",
    "    def setIG(self, ig):\n",
    "        self.data[5] = ig;\n",
    "    \n",
    "    def getSeparation(self,nbclasses,colname,edgename):\n",
    "        text = \"(\";\n",
    "        x = self.getLabels();\n",
    "        elx = np.unique(x);\n",
    "        for t in range(nbclasses-1):\n",
    "            text += str(len(x[x == t]))+edgename[len(colname)-1][t][0]+\",\";\n",
    "            \n",
    "        text += str(len(x[x == nbclasses-1]))+edgename[len(colname)-1][nbclasses-1][0]+\")\";\n",
    "        return text;\n",
    "    \n",
    "    def show(self):\n",
    "        Q = queue.Queue(maxsize=20);\n",
    "        Q.put(self);\n",
    "        level = 0;\n",
    "        k = 1;\n",
    "        ck = 1;\n",
    "        while(Q.qsize() != 0):\n",
    "            x = Q.get();\n",
    "            \n",
    "            if(x != '|'):\n",
    "                print(x.getName(),end=\" \");\n",
    "                for t in x.children:\n",
    "                    Q.put(t);\n",
    "\n",
    "                if (len(x.children) != 0):\n",
    "                    Q.put('|');\n",
    "                \n",
    "                if(k == 1):\n",
    "                    k = Q.qsize()-ck;\n",
    "                    ck = k;\n",
    "                    i = 0;\n",
    "                    level+=1;\n",
    "                    print('\\n')\n",
    "                else:\n",
    "                    k-=1;\n",
    "            else:\n",
    "                print(x,end=\" \")\n",
    "                \n",
    "    def showRender(self, colname, edgename,nbclasses,file):\n",
    "        Q = queue.Queue(maxsize=200);\n",
    "        Q.put(self);\n",
    "        Q1 = queue.Queue(maxsize=200);\n",
    "        tree = Node(self.getSeparation(nbclasses,colname,edgename)+\"\\n\"+colname[self.getColumnIndex()]+'\\n'+str('%.4f'%self.getIG()));\n",
    "        Q1.put(tree);    \n",
    "        i = 0;\n",
    "        while(Q.qsize() != 0):\n",
    "            x = Q.get();\n",
    "            actpar = Q1.get();\n",
    "            for t in x.children:\n",
    "                i+=1;\n",
    "                Q.put(t);\n",
    "                \n",
    "                if(t.getColumnIndex() < 0):\n",
    "                    tag = str(edgename[len(colname)-1][-t.getColumnIndex()-1]);\n",
    "                else:\n",
    "                    tag = colname[t.getColumnIndex()];\n",
    "                    \n",
    "                nod = Node(t.getSeparation(nbclasses,colname,edgename)+\"\\n\"+tag+'\\n'+str('%.4f'%t.getIG())+'\\n'+str(i),parent = actpar,edge=edgename[x.getColumnIndex()][t.getBranch()]);\n",
    "                Q1.put(nod);\n",
    "                \n",
    "        RenderTreeGraph(tree,nodenamefunc=nodenamefunc,edgeattrfunc=edgeattrfunc).to_picture(file);\n",
    "        #RenderTreeGraph(tree).to_dotfile(\"tree.dot\")\n",
    "        #print(RenderTree(tree).by_attr());       \n",
    "            "
